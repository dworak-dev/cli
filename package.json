{
  "name": "@dworac/cli",
  "description": "Collection of dworac's CLI tools.",
  "version": "2.1.0",
  "author": "dworac <mail@dworac.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js --sourcemap --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "bin": {
    "cli": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dworac/typescript-starter.git"
  },
  "keywords": [
    "cli",
    "typescript",
    "dworac"
  ],
  "bugs": {
    "url": "https://github.com/dworac/typescript-starter/issues"
  },
  "homepage": "https://github.com/dworac/typescript-starter#readme",
  "dependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/node": "^18.14.6",
    "chalk": "^2.4.2",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "commander": "^10.0.0",
    "esbuild": "^0.17.11",
    "fs-extra": "^11.1.0",
    "inquirer": "8.0.0",
    "variable-replacer": "^1.6.0"
  },
  "devDependencies": {
    "@dworac/eslint-config-typescript": "^1.2.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist"
  ]
